{
  "name": "plutarch",
  "version": "1.0.0",
  "description": "Cli tool for serve and build react app, support JSON pattern config.",
  "main": "index.js",
  "bin": {
    "plutarch": "bin/plutarch.js"
  },
  "keywords": [
    "plutarch",
    "react",
    "cli",
    "create-react-app",
    "webpack"
  ],
  "files": [
    "bin",
    "src",
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "lint": "eslint --ext .js src",
    "test": "npm run lint && npm run build && cross-env NODE_ENV=test nyc mocha --no-timeouts test/**/*-test.js",
    "debug": "cross-env NODE_ENV=test mocha --require babel-register --no-timeouts test/**/*-test.js"
  },
  "author": "alfred <sc941203@gmail.com> (https://github.com/schifred)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schifred/plutarch/issues"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": false
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "common-bin": "^2.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "csv-loader": "^2.1.1",
    "debug": "^3.1.0",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "i18n-webpack-plugin": "^1.0.0",
    "invariant": "^2.2.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "mocha": "^4.0.0",
    "node-sass": "^4.5.3",
    "npm-install-webpack-plugin": "^4.0.5",
    "react-dev-utils": "^4.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "xml-loader": "^1.2.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "expect": "^21.2.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2"
  }
}

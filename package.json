{
  "name": "plutarch",
  "version": "2.0.1",
  "description": "Cli tool for serve and build react app, support JSON pattern config.",
  "main": "index.js",
  "bin": {
    "plutarch": "bin/plutarch.js"
  },
  "keywords": [
    "plutarch",
    "react",
    "cli",
    "create-react-app",
    "webpack"
  ],
  "files": [
    "bin",
    "src",
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "lint": "eslint --ext .js src",
    "test": "npm run lint && npm run build && cross-env NODE_ENV=test nyc mocha --no-timeouts test/**/*-test.js",
    "debug": "cross-env NODE_ENV=test mocha --require babel-register --no-timeouts test/**/*-test.js"
  },
  "author": "alfred <sc941203@gmail.com> (https://github.com/schifred)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schifred/plutarch/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/schifred/plutarch.git"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": false
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "chalk": "^2.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "common-bin": "^2.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-decorators": "^0.20.0",
    "css-loader": "^0.28.7",
    "csv-loader": "^2.1.1",
    "debug": "^3.1.0",
    "eslint": "4.19.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.10",
    "glob": "^7.1.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.1.0",
    "i18n-webpack-plugin": "^1.0.0",
    "invariant": "^2.2.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "4.17.4",
    "mocha": "^4.0.0",
    "postcss-loader": "^2.0.6",
    "react-app-rewired": "^1.4.1",
    "react-dev-utils": "^5.0.0",
    "react-error-overlay": "^4.0.0",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.1",
    "style-loader": "^0.18.2",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "4.1.0",
    "webpack-dev-server": "3.1.0",
    "xml-loader": "^1.2.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "expect": "^21.2.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2"
  }
}

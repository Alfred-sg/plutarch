  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-import": "^1.12.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.1",
    "mobx-react-devtools": "^6.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.0",
    "eslint": "^6.1.0",
    "eslint-config-alloy": "^2.0.5",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1"
  }
}

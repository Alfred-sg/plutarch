{
  "name": "mobx-demo",
  "version": "1.0.0",
  "description": "A application demo built upon Ant Design and Mobx.js",
  "main": "index.js",
  "scripts": {
    "dev": "plutarch server",
    "build": "plutarch build",
    "lint": "eslint --ext **/*.js src",
    "lint:fix": "eslint --fix **/*.js src"
  },
  "husky": {
